{
  "name": "electron-forge-webpack-ts-starter",
  "version": "0.0.0",
  "description": "out-of-the-box starter structure for electron-forge w/ webpack & typescript",
  "keywords": [],
  "license": "MIT",
  "author": "G-Rath",
  "main": "./.webpack/main",
  "scripts": {
    "check:all": "npm run check:renderer && npm run check:main",
    "check:main": "tsc --noEmit -p ./src/main",
    "check:renderer": "tsc --noEmit -p ./src/renderer",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "electron-forge start"
  },
  "config": {
    "forge": {
      "bundleIdentifier": "elastic-library",
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-forge-webpack-ts-starter"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./config/webpack.main.config.js",
            "renderer": {
              "config": "./config/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/redux-immutable": "^4.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.24.0",
    "numeraljs": "^1.5.6",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "redux-immutable": "^4.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.32",
    "@electron-forge/maker-deb": "6.0.0-beta.32",
    "@electron-forge/maker-rpm": "6.0.0-beta.32",
    "@electron-forge/maker-squirrel": "6.0.0-beta.32",
    "@electron-forge/maker-zip": "6.0.0-beta.32",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.22",
    "@types/classnames": "^2.2.7",
    "@types/elasticsearch": "^5.0.31",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/webpack": "^4.4.24",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.6.0",
    "elasticsearch": "^15.4.1",
    "electron": "4.0.4",
    "file-loader": "^3.0.1",
    "jquery": "^3.3.1",
    "react-notifications": "^1.4.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.1",
    "webpack": "^4.29.5",
    "webpack-merge": "^4.2.1"
  },
  "productName": "electron-forge-webpack-ts-starter"
}
